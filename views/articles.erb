<h1>Your News</h1>

    <% @articles.each do |article| %>
    <ul>
      <li><%= article.title %> </li>
      <li> <%= article.description %> </li>
      <li> <a href='<%= article.url %>' target = '_blank' >Read Article <%= article.id %></a></li>
    </ul>
    <% end %>
     <a href="/articles/new">Add a new article</a>
     <a href="/articles/random">See a Random article</a>
